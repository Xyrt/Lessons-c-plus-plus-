#include "BinaryTree.h"
#include <iostream>

int main()
{
	BinaryTree tree;
	tree.Insert(5);
	tree.Insert(10);
	tree.Insert(3);
	tree.Insert(6);
	tree.Insert(0);
	tree.Print();

	TreeNode* node = tree.Find(3);
	tree.Erase(node);
	std::cout << "\n";
	tree.Print();

	if (node != nullptr)
	{
		std::cout << "Found! whooHooo\n";
	}
}
